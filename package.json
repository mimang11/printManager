{
  "name": "printmanager",
  "version": "1.0.0",
  "main": "dist/main/main/index.js",
  "description": "打印机 BI 管理系统",
  "author": "User",
  "license": "MIT",
  "scripts": {
    "dev:renderer": "vite",
    "dev:main": "tsc -p tsconfig.main.json",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:5173 && npm run dev:main && cross-env NODE_ENV=development electron .\"",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "build": "npm run build:renderer && npm run build:main",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.printmanager.app",
    "productName": "打印机管理系统",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "target": "portable",
      "signAndEditExecutable": false,
      "icon": "build/icon.jpg"
    },
    "portable": {
      "artifactName": "PrinterManager.exe"
    }
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "fs-extra": "^11.3.3",
    "iconv-lite": "^0.7.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "mammoth": "^1.11.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "wait-on": "^9.0.3"
  }
}
